<AnchorPane xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1" fx:controller="rocket.app.view.MortgageController">
   <children>
      <GridPane prefHeight="353.0" prefWidth="472.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <TextField fx:id="txtIncome" GridPane.columnIndex="1" />
            <TextField fx:id="txtExpenses" GridPane.columnIndex="1" GridPane.rowIndex="1" />
            <TextField fx:id="txtCreditScore" GridPane.columnIndex="1" GridPane.rowIndex="2" />
            <TextField fx:id="txtHouseCost" GridPane.columnIndex="1" GridPane.rowIndex="3" />
            <Label fx:id="lblIncome" prefHeight="49.0" prefWidth="78.0" text="Income" />
            <Label fx:id="lblExpenses" prefHeight="49.0" prefWidth="96.0" text="Expenses" GridPane.rowIndex="1" />
            <Label fx:id="lblCreditScore" prefHeight="58.0" prefWidth="99.0" text="Credit Score" GridPane.rowIndex="2" />
            <Label fx:id="lblHouseCost" prefHeight="54.0" prefWidth="99.0" text="Cost of House" GridPane.rowIndex="3" />
            <Label fx:id="lblLoanTerm" prefHeight="57.0" prefWidth="98.0" text="Loan Term (yr)" GridPane.columnIndex="2" />
            <ComboBox fx:id="cboxLoanTerm" prefWidth="150.0" GridPane.columnIndex="3" />
            <Label fx:id="lblPayment0" text="Mortgage Payment" GridPane.columnIndex="2" GridPane.rowIndex="2" />
            <Label fx:id="lblErrors0" text="Error Messages" GridPane.columnIndex="2" GridPane.rowIndex="3" />
            <Button fx:id="calcButton" mnemonicParsing="false" onAction="#btnCalculatePayment" prefHeight="5.0" prefWidth="128.0" text="Calculate" GridPane.columnIndex="2" GridPane.rowIndex="1" />
            <Label fx:id="lblPayment" GridPane.columnIndex="3" GridPane.rowIndex="2" />
            <Label fx:id="lblErrors" GridPane.columnIndex="3" GridPane.rowIndex="3" />
         </children>
      </GridPane>
   </children>
